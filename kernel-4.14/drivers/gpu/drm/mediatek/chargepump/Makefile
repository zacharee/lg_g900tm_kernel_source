# CONFIG_CUSTOM_KERNEL_CHARGEPUMP is set to y
# Add driver list what you want to use in CONFIG_CUSTOM_KERNEL_CHARGEPUMP_LIST

ifeq ($(CONFIG_JDM_LCM),y)
obj-$(CONFIG_CUSTOM_KERNEL_CHARGEPUMP) += jdm/
else
CUSTOM_KERNEL_CHARGEPUMP_LIST := $(subst ",,$(CONFIG_CUSTOM_KERNEL_CHARGEPUMP_LIST))
CHARGEPUMP_LISTS := $(CUSTOM_KERNEL_CHARGEPUMP_LIST)
obj-$(CONFIG_CUSTOM_KERNEL_CHARGEPUMP) += $(foreach CHARGEPUMP,$(CHARGEPUMP_LISTS),$(CHARGEPUMP)/)
endif
